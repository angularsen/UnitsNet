        {
          "Culture": "ru-RU",
          "Abbreviations": [ "т" ]
        },
        {
          "Culture": "zh-CN",
          "Abbreviations": [ "吨" ]
        }
      ]
    },
    {
      "SingularName": "ShortTon",
      "PluralName": "ShortTons",
      "BaseUnits": {
        "M": "ShortTon"
      },
      "XmlDocSummary": "The short ton is a unit of mass equal to 2,000 pounds (907.18474 kg), that is most commonly used in the United States – known there simply as the ton.",
